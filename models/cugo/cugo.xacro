      <material name="dummy_white"/>
    </visual>
    <collision>
      <geometry>
        <!--<cylinder length="0.1" radius="0.03858"/>-->
        <!--<cylinder length="0.169" radius="0.01929"/>-->
        <cylinder length="${collision_wheel_length}" radius="${mini_wheel_radius}"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.00024" iyy="0.00024" izz="0.00015" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
  </link>



