    <plugin name="cugo_diff_drive" filename="libgazebo_ros_diff_drive.so">

      <update_rate>10</update_rate>

      <!-- wheels -->
      <left_joint>right_wheel_joint</left_joint>
      <right_joint>left_wheel_joint</right_joint>

      <!-- kinematics -->
      <wheel_separation>0.460</wheel_separation>
      <wheel_diameter>0.040</wheel_diameter>

      <!-- limits -->
      <max_wheel_acceleration>1.0</max_wheel_acceleration>
      <max_wheel_torque>1.0</max_wheel_torque>

      <command_topic>cmd_vel</command_topic>

      <!-- output -->
      <publish_odom>true</publish_odom>
      <publish_odom_tf>true</publish_odom_tf>
      <publish_wheel_tf>false</publish_wheel_tf>
      <!--<publish_wheel_joint_state>true</publish_wheel_joint_state>-->

      <odometry_topic>odom</odometry_topic>
      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_footprint</robot_base_frame>
      <!--<robot_base_frame>base_link</robot_base_frame>-->

      <odometry_source>1</odometry_source>
      <!--<legacy_mode>false</legacy_mode>-->
    </plugin>
  </gazebo>

