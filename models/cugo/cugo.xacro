  <xacro:include filename="$(find cugo_ros2_control)/models/cugo/gazebo_material_macro.xacro" />

  <xacro:property name="body_width" value="0.42" />
  <xacro:property name="body_depth" value="0.58" />
  <xacro:property name="body_height" value="0.26" />

  <xacro:property name="wheel_length" value="0.06" />
  <!--<xacro:property name="wheel_length" value="0.145" />-->
  <xacro:property name="collision_wheel_length" value="0.145" />
  <!-- これは本当の車輪径 -->
  <!-- <xacro:property name="wheel_radius" value="0.03858" /> -->
  <!-- 当たり判定の径 -->
  <xacro:property name="wheel_radius" value="0.050" />
  <!-- スプロケット径 -->
  <!-- <xacro:property name="mini_wheel_radius" value="0.01929" /> -->
  <xacro:property name="mini_wheel_radius" value="0.040" />

  <xacro:property name="big_wheel_front_y" value="0.167" />
  <xacro:property name="big_wheel_back_y" value="-0.167" />


  <xacro:property name="center_wheel_z" value="-0.105" />
  <xacro:property name="big_wheel_z" value="-0.08" />

  <xacro:property name="right_wheel_x" value="0.24" />

  <xacro:property name="left_wheel_x" value="-0.22" />



  <link name="base_footprint"/>

  <joint name="base_footprint_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.1065" rpy="0 0 0"/>
  </joint>

  <!-- base_link -->
  <link name="base_link"/>

  <joint name="body_joint" type="fixed">
    <parent link="base_link"/>
    <child  link="base_body"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>

  <!-- 箱 -->
  <link name="base_body">
    <visual>
      <geometry>
        <!--<box size="0.58 0.58 0.3"/>-->
        <!--<box size="${body_depth} ${body_width} ${body_height}"/>-->
        <!--<mesh filename="file://$(find cugo_ros2_control)/urdf/xacro/roskit-Compound.dae" scale="1 1 1"/>-->
        <mesh filename="file://$(find cugo_ros2_control)/models/cugo/meshes/roskit-Compound.dae" scale="1 1 1"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 -1.57"/>
      <material name="red"/>
    </visual>
    <collision>
      <geometry>
    <!--
        <mesh filename="file://$(find cugo_ros2_control)/urdf/xacro/roskit-Compound.dae" scale="1 1 1"/>
    -->
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.13542" iyy="0.10833" izz="0.17708" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
  </link>

  <!-- 車輪のジョイントと車輪のリンク -->
  <joint name="right_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 0 -1"/>
    <limit lower="-1.57" upper="1.57" effort="0.5" velocity="2.0"/>
    <parent link="base_body"/>
    <child link="right_wheel"/>
    <!--<origin rpy="0 0 0" xyz="-0.133333333333 0 -0.085"/>-->
    <!--<origin rpy="0 1.57075 0" xyz="0.230 0 -0.1"/>-->
    <!--<origin rpy="0 1.57075 0" xyz="0.115 0 -0.16142"/>-->
