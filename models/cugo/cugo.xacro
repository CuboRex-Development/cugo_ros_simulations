    <!--<origin rpy="0 1.57075 0" xyz="0.2055 0 -0.178935"/>-->

    <!--<origin rpy="0 1.57075 0" xyz="0.2055 0 -0.215"/>-->

    <!--<origin rpy="0 1.57075 0" xyz="${right_wheel_x} 0 ${center_wheel_z}"/>-->
    <origin rpy="0 1.57075 -1.57" xyz="0 ${right_wheel_x} ${center_wheel_z}"/>
  </joint>

  <link name="right_wheel">
    <visual>
      <!--<origin rpy="1.57075 0 0" xyz="0 0 0"/>-->
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!--<cylinder length="0.1" radius="0.03858"/>-->
        <!--<cylinder length="0.169" radius="0.01929"/>-->
        <cylinder length="${wheel_length}" radius="${mini_wheel_radius}"/>
